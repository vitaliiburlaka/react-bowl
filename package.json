{
  "name": "react-bowl",
  "version": "0.1.0",
  "description": "react-bowl 🍜 - An React Boilerplate",
  "repository": {
    "type": "git",
    "url": "git@github.com:vitaliiburlaka/react-bowl.git"
  },
  "main": "",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --progress --config webpack.config.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:format": "prettier \"**/*.{md,json,yml,css}\" --check",
    "lint:format:fix": "prettier \"**/*.{md,json,yml,css}\" --write",
    "lint:styles": "stylelint --ignore-path .gitignore --syntax scss **/*.scss",
    "lint:styles:fix": "stylelint --ignore-path .gitignore --syntax scss **/*.scss --fix",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false webpack --progress --config webpack.config.js",
    "analyze": "npm run build -- --env.analyze"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.16.0",
    "npm": ">=6"
  },
  "keywords": [
    "React",
    "Boilerplate",
    "Webpack",
    "Jest",
    "Sass",
    "SCSS"
  ],
  "author": "Vitalii Burlaka <vitalii.burlaka@gmail.com>",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "core-js": "^3.8.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.10.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{md,json,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run coverage"
    }
  },
  "browserslist": [
    "defaults",
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
