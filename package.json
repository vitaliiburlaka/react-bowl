{
  "name": "react-bowl",
  "version": "0.1.0",
  "description": "react-bowl 🍜 - An React Boilerplate",
  "repository": {
    "type": "git",
    "url": "git@github.com:vitaliiburlaka/react-bowl.git"
  },
  "main": "",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --progress --config webpack.config.js --open",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:format": "prettier \"**/*.{md,json,yml,css}\" --check",
    "lint:format:fix": "prettier \"**/*.{md,json,yml,css}\" --write",
    "lint:styles": "stylelint --ignore-path .gitignore --syntax scss **/*.scss",
    "lint:styles:fix": "stylelint --ignore-path .gitignore --syntax scss **/*.scss --fix",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false webpack --progress --config webpack.config.js",
    "analyze": "npm run build -- --env.analyze"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.x.x",
    "npm": ">=6"
  },
  "keywords": [
    "React",
    "Boilerplate",
    "Webpack",
    "Jest",
    "Sass",
    "SCSS"
  ],
  "author": "Vitalii Burlaka <vitalii.burlaka@gmail.com>",
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "core-js": "^3.21.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "autoprefixer": "^10.4.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.1",
    "react-refresh": "^0.12.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.10",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{md,json,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run coverage"
    }
  },
  "browserslist": [
    "defaults",
    ">0.2%",
    "not dead",
    "not ie > 0",
    "not op_mini all"
  ]
}
